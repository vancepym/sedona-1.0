**
** ConstFloat: float constant  
**    out should never be a link destination.
**
@niagaraIcon="module://icons/x16/control/numericPointWr.png"
class WriteFloat
  extends Component
{
  @summary
  @config
   property float in
  
  @summary
  @readonly
   property float out
  
  action void set(float x)
  {           
    in = x
  }
  
  virtual override void execute()
  {
    out = in
  }
  
  
}
