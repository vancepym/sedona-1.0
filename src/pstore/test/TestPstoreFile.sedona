**
** TestPstoreFile
**
class TestPstoreFile
  extends PstoreFile
{


  property int result

  action void verifyRead(int offset)
  {
    result = read(offset)
  }

  action void verifyWrite(int x)
  {
    int offset = x >> 8
    int val    = x & 0xff
    result = write(offset, val) ? 1 : 0
  }

}